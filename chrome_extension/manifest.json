{
  "name": "Course Search",
  "version": "1.0",
  "description": "Search the Princeton registrar for courses.",
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://ice.tigerapps.org/*"],
      "js": ["ext-base.js", "ext-all.js", "ice.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "http://ice.tigerapps.org/",
    "http://google.com"
  ]
}
